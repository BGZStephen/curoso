generator client {
  provider        = "prisma-client-js"
  engineType      = "binary"
  output          = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                   String                 @id @default(cuid()) @db.VarChar(30)
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  email                String                 @unique
  firstName            String                 
  lastName             String                 
  password             String
  lastLogin            DateTime                 
}